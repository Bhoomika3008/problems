def sum_of_n_numbers_iterative(n):
  
  sum = 0
  for i in range(1, n + 1):
    sum += i
  return sum

def sum_of_n_numbers_formula(n):
  
  return (n * (n + 1)) // 2

# Example usage
number = 5
print(f"Sum of first {number} natural numbers (iterative): {sum_of_n_numbers_iterative(number)}")
print(f"Sum of first {number} natural numbers (formula): {sum_of_n_numbers_formula(number)}")
