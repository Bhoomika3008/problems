function printMultiplicationTable(number) {
  console.log(`Multiplication Table for ${number}:`);
  for (let i = 1; i <= 10; i++) {
    const result = number * i;
    console.log(`${number} * ${i} = ${result}`);
  }
}

// Example usage:
printMultiplicationTable(5); // Prints the multiplication table of 5
printMultiplicationTable(7); // Prints the multiplication table of 7
